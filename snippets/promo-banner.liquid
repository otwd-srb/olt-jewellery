<div id="shopify-section-sidebar-promoimage" class="shopify-section">
<div class="widget widget_promo_img" data-section-id="{{ section.id }}" >
  <ul id="promo-carousel">
    {% if promo_img != blank %}
    <li>
      <a href="{{ section.settings.promo_link }}" title="{{ promo_title }}"> 
        <img height="450" width="285" src="{{ promo_img | img_url : '285x' }}" alt="{{ promo_title }}" title="{{ promo_title }}" />
      </a>
    </li>
    {% endif %}
  </ul>
</div>
</div>
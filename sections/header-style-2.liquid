<style>
  .header_2_wrapper{
    background-color:{{section.settings.color_menu_bg }};
  }
  .header_2 .fullscreen_header_toggle {
  	background-color:{{section.settings.vr_menu_bg_color }};
  }
  .header_2 .fullscreen_header_toggle .leftmenu #menu, 
  .header_2 .fullscreen_header_toggle .leftmenu #menu::after, 
  .header_2 .fullscreen_header_toggle .leftmenu #menu::before {
    background: {{ section.settings.vr_menu_text_color }};
  }
  .header_2 .fullscreen_header_toggle .menu-title {
  	color: {{ section.settings.vr_menu_text_color }};
  }
  .header_2 .fullscreen_header_toggle
  .header_2 .right-link-icon .myaccount,
  .header_2 .right-link-icon .site-header__search.icon-btn{
    color:{{section.settings.main_txt_header }};
  }
  .header_2 .site-header__cart-count .cart-products-count,
  .header_2 .wishlist-icon-div .wishlist-count {
    color:{{section.settings.count_color }};
    background:{{section.settings.countbgcolor }};
  }
  .header_2 .wrapper-top-cart > .site-header_cart_link svg {
    fill:{{section.settings.main_txt_header }};
  }
  .header_2 .wishlist-icon-div a > i,
  .header_2 .site-header__search .serach_icon > i,
  .header_2 .menu_toggle_wrap > button {
  	color: {{section.settings.main_txt_header }};
  }
  .header_2 .offer-content{color:{{section.settings.offer_text_color }};}
  .header_2 .header-top{background-color:{{section.settings.top_bg_color}};color:{{section.settings.top_header_text_color}}}
  .header_2 #tt-megamenu1 .tt-mega_menu1 li.tt_menu_item > a::before,
  .header_2.site-header #tt-megamenu .tt-mega_menu .tt_menus_ul > li > a::before {
  	background-color: {{ section.settings.menu_bg_color }};
  }
  .header_2_wrapper .main-header.fixed {
  	background-color:{{section.settings.color_menu_bg }};
  }
  @media (min-width: 992px) {
    .header_2 #tt-megamenu .tt-mega_menu li.tt_menu_item > a,
    .header_2 #tt-megamenu .tt-mega_menu li.tt_menu_item span.categories {
      color: {{ section.settings.menu_text_color }}
    }
    .header_2 #tt-megamenu .tt-mega_menu li.tt_menu_item:hover > a,
    .header_2 #tt-megamenu .tt-mega_menu li.tt_menu_item:hover span.categories {
      background-color: {{ section.settings.menu_bg_color }};
      color: {{ section.settings.menu_text_hover_color }};
    }
  }
   @media (max-width: 991px) {
        .header_2_wrapper .right-nav .myaccount span svg{
         fill:{{section.settings.main_txt_header }};        
        }
</style>
{% capture navSize %}250px{% endcapture %}
{% capture navHeaderSize %}{{ section.settings.menu_mobile }}px{% endcapture %}
{% capture logoMaxHeight %}{{ section.settings.menu_mobile | minus: 30 }}px{% endcapture %}
{% capture borderWidth %}{{ settings.border_width }}px{% endcapture %}
{% capture colorBorder %}{{ settings.color_borders }}{% endcapture %}
{% assign navMenu = linklists[section.settings.nav_menu] %}
{% if section.settings.sticky_header %}{%- assign sticky_class = 'sticky_header' -%}{% endif %}
<header class="site-header page-element header_2{% if section.settings.announcement_bar %} bar{% endif %}" data-section-id="{{ section.id }}" data-section-type="header-section">
  <div class="header_2_wrapper">
    {% if section.settings.announcement_bar %}
    <div class="header-top">
      <div class="container">   
        <div class="destination"><a href="#shopify-section-footer-model-1">{{ section.settings.destination }}</a></div>
      </div>
    </div>
    {% endif %}
    <div class="header-middle">
      <div class="container">     
        <div class="middle-content">
          <div class="left-nav">
            <div class="header-contact">
            <a href="tel:{{ section.settings.contact_number }}">
            <i class="mdi mdi-phone-in-talk"></i>
            <div class="contact-number">{{ section.settings.contact_number }}</div>
            </a>
            </div>
            <div class="header-email">
              <a href="mailto:{{ section.settings.email }}">
                <i class="mdi mdi-email-open"></i>
                <div class="email">{{ section.settings.email }}</div>
              </a>
            </div> 
          </div>
          <div class="right-nav">
            {% if section.settings.display_myaccount %}
            <div class="myaccount icon-btn">
              <span class="dropdown-toggle" title="My Account">
                <svg version="1.1" id="Capa_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
                     viewBox="0 0 412.333 412.333" style="enable-background:new 0 0 412.333 412.333;" xml:space="preserve" height="18" width="16">
                  <g>
                    <path d="M206.167,205.333c56.61,0,102.667-46.056,102.667-102.667S262.777,0,206.167,0S103.5,46.056,103.5,102.667
                             S149.556,205.333,206.167,205.333z M206.167,40c34.554,0,62.667,28.112,62.667,62.667s-28.112,62.667-62.667,62.667
                             S143.5,137.221,143.5,102.667S171.612,40,206.167,40z"/>
                    <path d="M206.167,223.333c-93.187,0-169,75.813-169,169v20h338v-20C375.167,299.146,299.354,223.333,206.167,223.333z
                             M78.714,372.333c9.637-61.671,63.12-109,127.452-109s117.815,47.329,127.452,109H78.714z"/>
                  </g>
                </svg>
                <span class="account-title">{{ 'customer.account.title' | t }}</span>
                <i class="mdi mdi-chevron-down" aria-hidden="true"></i>
                <i class="mdi mdi-chevron-up" aria-hidden="true"></i>
              </span>
              <div class="customer_account">  
                <ul>
                  {% if shop.customer_accounts_enabled %}
                  {% if customer %}
                  <li>
                    <a href="/account" title="{{ 'customer.account.hello' | t }}">{{ 'customer.account.hello' | t }}<span class="username">{{ customer.first_name }}</span></a>
                  </li>
                  <li class="logout-link">
                    {{ 'layout.customer.log_out' | t | customer_logout_link }}
                  </li>
                  {% else %}
                  <li>
                    <a href="/account/login" title="{{ 'layout.customer.log_in' | t }}">{{ 'layout.customer.log_in' | t }}</a>
                  </li>
                  <li>
                    <a href="/account/register" title="{{ 'layout.customer.create_account' | t }}">{{ 'layout.customer.create_account' | t }}</a>
                  </li>          
                  {% endif %}  
                  {% endif %} 
                </ul>
              </div>
            </div>
            {% endif %}
            {% if section.settings.display_compare %}  
            <div class="compare-icon-div">
              <a class="compare compare-icon" href="{% if shop.locale != 'en' %}/{{ shop.locale }}{% endif %}/pages/compare" title="{{ 'products.compare.compare_name' | t }}">{{ 'products.compare.compare_name' | t }}(<span class="compare-count"></span>)</a>
            </div>
            {% endif %}
            {% if section.settings.display_language %} 
            <div class="header_language">
               <span class="title">{{ 'general.header.language' | t }}</span>
              <ul class="tbl-list">
                <li class="language dropdown-parent uppercase language-block"> 
                  {% render 'language-switcher' %}
                </li>
              </ul>
            </div>
            {% endif %}
            {% if section.settings.display_currency %} 
            <div class="header_currency">
               <span class="title">{{ 'general.header.currency' | t }}</span>
              <ul class="tbl-list">
                <li class="currency dropdown-parent uppercase currency-block">
                  {% if section.settings.display_currency %}    
                  {% render 'currencies-switcher' %}
                  {% endif %}
                </li>
              </ul>
            </div>
            {% endif %}  
          </div>
        </div>
      </div>
    </div>
    <div id="header-sticky" class="main-header {{ sticky_class }}">
      <div class="container">         
        <div class="menu_toggle_wrap">
          <button type="button" class="text-link nav-toggle" id="navToggle">
            <i class="mdi mdi-menu"></i>
            <span class="menu-text">{{ 'general.navigation.menu' | t }}</span>
          </button>
        </div>
        <div class="header_logo_wrap">
          {% if template.name == 'index' %}
          <h1 class="header-logo" itemscope itemtype="http://schema.org/Organization">
            {% else %}
            <div class="h1 header-logo" itemscope itemtype="http://schema.org/Organization">
              {% endif %}
              {% if section.settings.logo != blank %}
              <a href="{{ routes.root_url }}" itemprop="url">
                <img height="40" width="93" src="{{ section.settings.logo | img_url: '93x40' }}" alt="{{ shop.name }}" title="{{ shop.name }}" itemprop="logo">
              </a>
              {% else %}
              <a href="{{ routes.root_url }}" itemprop="url">{{ shop.name }}</a>
              {% endif %}
            </div>
            {% if template.name == 'index' %}
          </h1>
          {% else %}
        </div>
        {% endif %}
        <div class="right-icon">
          {% if section.settings.display_wishlist %}  
          <div class="wishlist-icon-div icon-btn">
            {% render 'wishlist' %} 
          </div>
          {% endif %} 
          {% if section.settings.display_cart %}  
          <div class="site-header__cart icon-btn">
            <div class="wrapper-top-cart">                 
              <a href="{{ routes.cart_url }}" id="cart-products-count" class="site-header_cart_link">                    
                <svg xmlns="http://www.w3.org/2000/svg" id="Capa_1" enable-background="new 0 0 512 512" height="512" viewBox="0 0 512 512" width="512"><g>
                  <path d="m452 120h-60.946c-7.945-67.478-65.477-120-135.054-120s-127.109 52.522-135.054 120h-60.946c-11.046 0-20 8.954-20 20v352c0 11.046 8.954 20 20 20h392c11.046 0 20-8.954 20-20v-352c0-11.046-8.954-20-20-20zm-196-80c47.484 0 87.019 34.655 94.659 80h-189.318c7.64-45.345 47.175-80 94.659-80zm176 432h-352v-312h40v60c0 11.046 8.954 20 20 20s20-8.954 20-20v-60h192v60c0 11.046 8.954 20 20 20s20-8.954 20-20v-60h40z"/>
                  </g></svg>
                {% if cart.item_count > 0 %}
                <div id="CartCount_sticky" class="site-header__cart-count" title="Cart">
                  <span  class="cart-products-count" aria-hidden="true">{{ cart.item_count }}</span>                 
                </div>
                {% else %}
                <div id="CartCount_sticky" class="site-header__cart-count">
                  <span class="cart-products-count">0</span>        
                </div>
                {% endif %}
              </a>
              <div id="slidedown-cart"> 
                <div class="mini-cart-wrap">
                  <!--  <h3>{{ 'cart.general.mini_cart_title' | t }}</h3>-->
                  <div class="no-items">
                    <p>{{ 'cart.general.empty' | t }}</p>
                  </div>
                  <div class="has-items">
                    <ul class="mini-products-list">  
                      {% for item in cart.items %}
                      <li class="item" id="cart-item-{{item.id}}">
                        <a href="{{item.url}}" title="{{item.title}}" class="product-image">
                          <img src="{{ item.image | product_img_url: 'small' }}" alt="{{item.title}}"></a>
                        <div class="product-details">
                          <p class="product-name"><a href="{{item.url}}">{{ item.product.title }}</a></p>
                          <div class="variants">{% if item.product.variants.size != 0 %}{% unless item.variant.title contains 'Default' %}{{ item.variant.title }}{% endunless %}{% endif %}</div>
                          <div class="cart-collateral"><span class="price">{{item.price | money}}</span> x {{item.quantity}}</div>
                        </div>
                        <a href="javascript:void(0)" title="Remove This Item" class="btn-remove"><span class="mdi mdi-close"></span></a>
                      </li>
                      {% endfor %}
                    </ul>
                    <div class="summary">                
                      <p class="total">
                        <span class="label">{{ 'cart.general.cart_total' | t }}</span>
                        <span class="price">{{cart.total_price | money}}</span> 
                      </p>
                    </div>
                    <div class="actions">
                      <button class="btn checkout-btn" onclick="window.location='/checkout'"><i class="mdi mdi-share"></i>{{ 'cart.general.checkout' | t }}</button>
                      <button class="btn text-cart" onclick="window.location='/cart'"><i class="mdi mdi-cart-outline"></i>{{ 'cart.general.view_cart' | t }}</button>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          {% endif %}
        </div>
        <div class="site-header__search icon-btn col-sm-5 search-full">
          <span class="serach_icon search_toggle"><i class="mdi mdi-magnify"></i> <i class="mdi mdi-window-close"></i>
          </span>
          <div class="search-button search-full-screen">
            <div class="full-search-wrapper">
              <span class="close-search"><i class="mdi mdi-close"></i></span>
              {% render 'search-bar' %}        
              <div class="search-results-wrapper">
                <div class="search-results">
                </div>
              </div>
            </div>
          </div>
        </div> 
        <div class="menu-top">
          <div class="vertical-menu">
          <div class="fullscreen_header_toggle">
            <div class="leftmenu">
              <button id="menu"></button></div>
              <span class="menu-title">Shop Categories</span>
          </div>
          {% if section.blocks.size > 0 %}
          <div id="tt-megamenu1" class="tt-megamenu1">
            <div class="tt-mega_menu1">
              <ul class="tt_menus_ul1">
                {% for block in section.blocks %}
                {% if block.type == 'tt_mm_menu1' %}
                {%- assign i1 = 0 -%}
                {%- assign ik1 = 0 -%}
                {%- assign i1 = i1 | plus: 1 -%}
                {% if i1 == 1 %}
                <li id="menu--{{ block.id }}" class="item tt_menu_item{% if block.settings.tt_mn_show_sub1 == true %} tt_mm_hassub{% endif %}{% if block.settings.tt_mn_show_sub1 == false %} single{% endif %}">
                  <a href="{{ block.settings.button_link1 }}">
                  <span id="menuicon-{{ block.id }}" class="icon"></span>
                    <span class="tt_menu_item_heading_title">
                      {{ block.settings.title1 }}
                    </span>
                  </a>
                  <style> #tt-megamenu1 .tt_menu_item span#menuicon-{{ block.id }}.icon{mask-image:url({{ block.settings.tt_sub_menu_icon | img_url: '30x30'}});-webkit-mask-image:url({{ block.settings.tt_sub_menu_icon | img_url: '30x30'}});}</style>
                  {% if block.settings.tt_mn_show_sub1 == true %}
                  <span class="mobile-nav__sublist-trigger">
                    <span class="mobile-nav__sublist-expand">
                      <i class="mdi mdi-chevron-down" aria-hidden="true"></i>
                    </span>
                    <span class="mobile-nav__sublist-contract">
                      <i class="mdi mdi-chevron-up" aria-hidden="true"></i>
                    </span>
                  </span>{% endif %}
                  <div class="tt_sub_menu_wrap {% if block.settings.tt_mn_show_sub1 == false %}disable{% endif %}">
                    {% endif %}
                    {%- assign ik1 = 0 -%}
                    {% endif %}
                    <div class="tt_sub_menu">
                      {% if block.settings.tt_sub_menu_ctitle1_show != blank %}
                      {% if block.settings.tt_sub_menu_clink1 != blank %}
                      {% if block.settings.tt_sub_menu_ctitle1 != blank %}
                      <h4 class="sub_mn_title">
                        <a href="{{ block.settings.tt_sub_menu_clink1 }}">{{ block.settings.tt_sub_menu_ctitle1 }}</a>
                      </h4>
                      {% endif %}
                      {% else %}
                      {% if block.settings.tt_sub_menu_ctitle1 != blank %}
                      <h4 class="sub_mn_title">{{ block.settings.tt_sub_menu_ctitle1 }}</h4>
                      {% endif %}
                      {% endif %}
                      {% endif %}
                      {%- assign tt_sub_menu_c1 = block.settings.tt_sub_menu_clinklist1 -%}
                      {%- if linklists[tt_sub_menu_c1].links != blank -%}
                      <ul class="tt_sub_menu_linklist">
                        {%- for link in linklists[tt_sub_menu_c1].links -%}
                        <li{% if link.links != blank %} class="tt_menu_item"{% endif %}>
                          <a href="{{ link.url }}" title="{{ link.object.description | strip_html }}">{{ link.title }}</a>
                          {% if link.links != blank %}
                          <div class="tt_sub_menu_wrap">
                            <ul class="sub_menu_linklist">
                              {% for link in link.links %}
                              <li>
                                <a href="{{ link.url }}" title="{{ link.object.description | strip_html }}">{{ link.title }}</a>
                              </li>
                              {% endfor %}
                            </ul>
                          </div>
                          {% endif %}
                        </li>
                        {% endfor %}
                      </ul>
                      {% endif %}
                    </div>
                    {%- assign ik1 = 1 -%}
                    {% endfor %}
                    {% if ik1 == 0 %}
                  </div>
                </li>
                {% endif %}
              </ul>
            </div>
          </div>
          {% endif %}
          </div>
        {% if section.blocks.size > 0 %}
        <div id="shopify-section-TT-megamenu" class="shopify-section tt-mega-menu">
          <div id="tt-megamenu" class="tt-megamenu">
            <div class="tt-mega_menu">
              <ul class="tt_menus_ul">
                {%- assign i = 0 -%}
                {%- assign ik = 0 -%}
                {% for block in section.blocks %}
                {%- assign i = i | plus: 1 -%}
                {% if block.type == 'tt_mm_menu' %}
                {% if i == 1 %}
                <li class="tt_menu_item{% if block.settings.tt_mn_show_sub == true %} tt_mm_hassub{% endif %}{% if block.settings.tt_mn_show_sub == false %} single{% endif %}{% if block.settings.tt_mn_class != blank %} {{ block.settings.tt_mn_class }}{% endif %}">
                  <a href="{{ block.settings.button_link }}">
                    <span class="tt_menu_item_heading_title">{{ block.settings.title }}
                      {% if block.settings.tt_mn_sub_heading != blank %}
                      <span class="tt_mn_sub_heading" style="{% if block.settings.tt_mn_subheading_bg != blank %}background-color:{{ block.settings.tt_mn_subheading_bg }};{% endif %}{% if block.settings.tt_mn_subheading_color != blank %}color:{{ block.settings.tt_mn_subheading_color }};{% endif %}">{{ block.settings.tt_mn_sub_heading }}</span>{% endif %}
                    </span>
                  </a>
                  {% if block.settings.tt_mn_show_sub == true %}  
                  <span class="mobile-nav__sublist-trigger">
                    <span class="mobile-nav__sublist-expand">
                      <i class="mdi mdi-chevron-down" aria-hidden="true"></i>
                    </span>
                    <span class="mobile-nav__sublist-contract">
                      <i class="mdi mdi-chevron-up" aria-hidden="true"></i>
                    </span>
                  </span>{% endif %}
                  <div class="tt_sub_menu_wrap {% if block.settings.tt_mn_show_sub == false %}disable{% endif %}" {% if block.settings.tt_mn_sub_contentwidth != blank %}style="width:{{ block.settings.tt_mn_sub_contentwidth }}"{% endif %} >
                    {% else %}
                  </div>
                </li>
                <li id="menu--{{ block.id }}" class="tt_menu_item{% if block.settings.tt_mn_show_sub == true %} tt_mm_hassub{% endif %}{% if block.settings.tt_mn_show_sub == false %} single{% endif %} {% if block.settings.tt_mn_class != blank %} {{ block.settings.tt_mn_class }}{% endif %}">
                  <a href="{{ block.settings.button_link }}">
                    <span class="tt_menu_item_heading_title">
                      {{ block.settings.title }}
                      {% if block.settings.tt_mn_sub_heading != blank %}
                      <span class="tt_mn_sub_heading" style="{% if block.settings.tt_mn_subheading_bg != blank %}background-color:{{ block.settings.tt_mn_subheading_bg }};{% endif %}{% if block.settings.tt_mn_subheading_color != blank %}color:{{ block.settings.tt_mn_subheading_color }};{% endif %}">{{ block.settings.tt_mn_sub_heading }}</span>{% endif %}
                    </span>
                  </a>
                  {% if block.settings.tt_mn_show_sub == true %}
                  <span class="mobile-nav__sublist-trigger">
                    <span class="mobile-nav__sublist-expand">
                      <i class="mdi mdi-chevron-down" aria-hidden="true"></i>
                    </span>
                    <span class="mobile-nav__sublist-contract">
                      <i class="mdi mdi-chevron-up" aria-hidden="true"></i>
                    </span>
                  </span>
                  {% endif %}
                  <div class="tt_sub_menu_wrap {% if block.settings.tt_mn_show_sub == false %}disable{% endif %}" {% if block.settings.tt_mn_sub_contentwidth != blank %}style="width:{{ block.settings.tt_mn_sub_contentwidth }}"{% endif %}>
                    {% endif %}
                    {%- assign ik = 0 -%}
                    {% endif %}


                    {% if block.settings.tt_mn_show_sub == true %}
                    <div class="tt_sub_menu" {% if block.settings.tt_sub_menu_cwidth != blank %}style="width:{{ block.settings.tt_sub_menu_cwidth }}"{% endif %}>

                      {% if block.settings.tt_sub_menu_ctitle != blank %}
                      <h4 class="sub_mn_title">
                        <a href="{{ block.settings.tt_sub_menu_clink }}">{{ block.settings.tt_sub_menu_ctitle }}</a>
                      </h4>
                      {% endif %}


                      {%- assign tt_sub_menu_c = block.settings.tt_sub_menu_clinklist -%}


                      {%- if linklists[tt_sub_menu_c].links != blank -%}
                      <ul class="tt_sub_menu_linklist">
                        {%- for link in linklists[tt_sub_menu_c].links -%}
                        <li{% if link.links != blank %} class="tt_menu_item"{% endif %}>
                          <a href="{{ link.url }}" title="{{ link.object.description | strip_html }}">{{ link.title }}</a>
                          {% if link.links != blank %}
                          <div class="tt_sub_menu_wrap">
                            <ul class="sub_menu_linklist">
                              {% for link in link.links %}
                              <li>
                                <a href="{{ link.url }}" title="{{ link.object.description | strip_html }}">{{ link.title }}</a>
                              </li>
                              {% endfor %}
                            </ul>
                          </div>
                          {% endif %}
                        </li>
                        {% endfor %}
                      </ul>
                      {% endif %}



                      {% if block.settings.tt_sub_menu_csecondtitle != blank %}
                      <h4 class="sub_mn_title">
                        <a href="{{ block.settings.tt_sub_menu_csecondlink }}">{{ block.settings.tt_sub_menu_csecondtitle }}</a>
                      </h4>
                      {% endif %}


                      {%- assign tt_sub_menu_csecondmenu = block.settings.tt_sub_menu_csecondlinklist -%}

                      {%- if linklists[tt_sub_menu_csecondmenu].links != blank -%}
                      <ul class="tt_sub_menu_linklist">
                        {%- for link in linklists[tt_sub_menu_csecondmenu].links -%}
                        <li>
                          <a href="{{ link.url }}" title="{{ link.object.description | strip_html }}">{{ link.title }}</a>
                          {% if link[tt_sub_menu_csecondmenu].links != blank %}
                          <ul class="tt_sub_menu_linklist">
                            {%- for sub_link in link[tt_sub_menu_csecondmenu].links -%}
                            <li>
                              <a href="{{ sub_link.url }}" title="{{ sub_link.object.description | strip_html }}">{{ sub_link.title }}</a>
                            </li>
                            {% endfor %}
                          </ul>
                          {% endif %}
                        </li>
                        {% endfor %}
                      </ul>
                      {% endif %}
                    </div>

                    <div class="tt_sub_menu" {% if block.settings.tt_sub_menu_cwidth != blank %}style="width:{{ block.settings.tt_sub_menu_cwidth }}"{% endif %}>
                      {% if block.settings.tt_sub_menu_cthirdtitle != blank %}
                      <h4 class="sub_mn_title">
                        <a href="{{ block.settings.tt_sub_menu_cthirdlink }}">{{ block.settings.tt_sub_menu_cthirdtitle }}</a>
                      </h4>
                      {% endif %}


                      {%- assign tt_sub_menu_cthirdmenu = block.settings.tt_sub_menu_cthirdlinklist -%}

                      {%- if linklists[tt_sub_menu_cthirdmenu].links != blank -%}
                      <ul class="tt_sub_menu_linklist">
                        {%- for link in linklists[tt_sub_menu_cthirdmenu].links -%}
                        <li>

<script>
  if(jQuery(window).width() >= 992) {
    function moremenu1() {     
      var max_elem = {{ section.settings.more_menu_category }};
      var more_info = "{{ 'general.header.more' | t }}";
      var items = jQuery('.header_style_2 .tt_menus_ul > li');
      var surplus = items.slice(max_elem, items.length);
      surplus.wrapAll('<li class="more_menu tt_menu_item left"><ul class="tt_sub_menu_linklist tt_sub_menu_wrap">');
      jQuery('.header_style_2 .more_menu').prepend('<a href="#"><span class="tt_menu_item_heading_title">'+more_info+'<span></a>');
      jQuery('.header_style_2 .more_menu').mouseover(function(){
        jQuery(this).children('ul').addClass('shown-link');
      })
      jQuery('.header_style_2 .more_menu').mouseout(function(){
        jQuery(this).children('ul').removeClass('shown-link');
      });
    }

    jQuery(document).ready(function(){
      moremenu1();
      jQuery(".header_style_2 #shopify-section-TT-megamenu").css("display", "inline-block");
    });

    function moremenu11() {     
      var max_elem = 8;
      var more_info = "{{ 'general.header.more' | t }}";
      var items = jQuery('.header_style_2 .tt_menus_ul1 > li');
      var surplus = items.slice(max_elem, items.length);
      surplus.wrapAll('<li class="more_menu1 tt_menu_item tt_mm_hassub left"><ul class="tt_sub_menu_linklist tt_sub_menu_wrap">');
      jQuery('.header_style_2 .more_menu1').prepend('<a href="#"><span class="tt_menu_item_heading_title">'+more_info+'<span></a>');
      if(jQuery(window).width() >= 992) {
        jQuery(".header_2 .tt_menus_ul1 > .more_menu1 .tt_menu_item.item").click(function(e) {
          jQuery(".header_2 #tt-megamenu1 .tt-mega_menu1 ul > li.tt_menu_item.more_menu1.active > ul.tt_sub_menu_linklist").stop(); 
          e.preventDefault();
        });
        jQuery('.header_2 .tt_menus_ul1 > .tt_menu_item.more_menu1').addClass('active');
        jQuery(".header_2 .tt_menus_ul1 > .tt_menu_item.more_menu1 .tt_sub_menu_linklist > li.tt_menu_item").addClass('open-nav');
        jQuery(".header_2 .tt_menus_ul1 > .tt_menu_item.more_menu1.active > a").click(function(e){
          jQuery(".header_2 .tt_menus_ul1 > .tt_menu_item.more_menu1").toggleClass('open');
          jQuery(".header_2 #tt-megamenu1 .tt-mega_menu1 ul > li.tt_menu_item.more_menu1.active > ul.tt_sub_menu_linklist.tt_sub_menu_wrap").slideToggle(300);
          e.preventDefault();
        });
        jQuery(".header_2 .tt_menus_ul1 > .tt_menu_item.more_menu1.active .tt_sub_menu_linklist > li.tt_menu_item .open-nav").click(function(e){
          jQuery(".header_2 .tt_menus_ul1 > .tt_menu_item.more_menu1.active .tt_sub_menu_linklist > li.tt_menu_item .open-nav").toggleClass('open');
          jQuery(".header_2 #tt-megamenu1 .tt-mega_menu1 ul > li.tt_menu_item.more_menu1.active > ul.tt_sub_menu_linklist > li.tt_menu_item .open-nav > .tt_sub_menu_wrap").slideToggle(300); 
          e.preventDefault();
        });
      }
    }
    jQuery(document).ready(function(){
      moremenu11();
      jQuery(".header_style_2 #shopify-section-TT-megamenu1").css("display", "inline-block");
    });

  }
</script>